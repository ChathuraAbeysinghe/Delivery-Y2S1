<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

     <servlet>
     	<servlet-name>deliveryInsert</servlet-name>
     	<servlet-class>Delivery.deliveryInsert</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>deliveryInsert</servlet-name>
     	<url-pattern>/Insert</url-pattern>
     </servlet-mapping>
     
     
      <servlet>
     	<servlet-name>showServlet</servlet-name>
     	<servlet-class>Delivery.showServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>showServlet</servlet-name>
     	<url-pattern>/show</url-pattern>
     </servlet-mapping>
     
     
      <servlet>
     	<servlet-name>updateStatusServlet</servlet-name>
     	<servlet-class>Delivery.updateStatusServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>updateStatusServlet</servlet-name>
     	<url-pattern>/update</url-pattern>
     </servlet-mapping>
     
     
     <servlet>
     	<servlet-name>deliveryDeleteServlet</servlet-name>
     	<servlet-class>Delivery.deliveryDeleteServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>deliveryDeleteServlet</servlet-name>
     	<url-pattern>/delete</url-pattern>
     </servlet-mapping>
   
 
     
</web-app>